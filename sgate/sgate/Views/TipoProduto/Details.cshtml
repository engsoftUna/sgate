@model sgate.Models.tipoproduto

@{
    ViewBag.Title = "Tipo de Produto";
}

<h2>Detalhe dos Tipos de Produtos</h2>

<fieldset>
    <legend>Detalhe dos Tipos de Produtos</legend>

    <div class="display-label">
         @Html.HiddenFor(model => model.idtipo)
    </div>
    <div class="display-field">
        @Html.HiddenFor(model => model.idtipo)
    </div>

    <div class="display-label">
         @Html.DisplayNameFor(model => model.descricao)
    </div>
    <div class="display-field">
        @Html.DisplayFor(model => model.descricao)
    </div>
</fieldset>
<p>
    @Html.ActionLink("Alterar", "Edit", new { id = Model.idtipo }) |
    @Html.ActionLink("Voltar", "Index")
</p>
